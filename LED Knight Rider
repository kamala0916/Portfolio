int del=100; // sets a default delay time
void setup() {
  // initialize the digital pins as outputs:
  for (int i = 2; i<=8 ; i++) {
    pinMode(i, OUTPUT);
  } // end of for loop
} // end of setup

void loop() {
  for (int i = 0; i<=13; i++) { // blink from LEDs 0 to 13
    digitalWrite(i, HIGH);
    delay(del);
    digitalWrite(i, LOW);
  }
  for (int i = 13; i>=0; i--) { // blink from LEDs 13 to 0
    digitalWrite(i, HIGH);
    delay(del);
    digitalWrite(i, LOW);
